<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="image">
	
			<select id="selectImage" resultType="Image" parameterType="int">
			select image
			from images
			where no=#{siteNo}
			</select>
			
	
</mapper>